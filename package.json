{
    "name": "painter",
    "version": "0.1.0",
    "license": "UNLICENSED",
    "browserslist": "> 0.5%, last 2 versions, not dead",
    "scripts": {
        "start": "parcel src/index.html",
        "build-dev": "echo 'error'",
        "build-prod": "echo 'error'",
        "typecheck": "tsc --project tsconfig.json --noEmit",
        "test": "ava",
        "generate-css": "tsm src --nameFormat \"camel\""
    },
    "ava": {
        "require": [
            "esm",
            "ts-node/register"
        ],
        "compileEnhancements": false,
        "extensions": [
            "ts"
        ],
        "files": [
            "**/*.test.ts"
        ],
        "sources": [
            "src"
        ]
    },
    "dependencies": {
        "@parcel/packager-ts": "^2.0.0",
        "@parcel/transformer-sass": "2.0.0",
        "@parcel/transformer-typescript-tsc": "2.0.0",
        "@parcel/transformer-typescript-types": "2.0.0",
        "@types/node": "16.11.1",
        "ava": "3.15.0",
        "color": "git+https://github.com/karifrederiksen/color.git",
        "esm": "3.2.25",
        "html-webpack-plugin": "5.4.0",
        "ivi": "1.0.1",
        "ivi-html": "1.0.0",
        "ivi-svg": "1.0.0",
        "jsverify": "0.8.4",
        "ordered-collections": "git+https://github.com/karifrederiksen/ordered-collections.git",
        "parcel": "2.0.0",
        "rxjs": "7.4.0",
        "ts-loader": "9.2.6",
        "ts-node": "10.3.0",
        "typed-scss-modules": "4.1.1"
    },
    "devDependencies": {
        "autoprefixer": "10.3.7",
        "postcss": "8.3.9",
        "precss": "4.0.0",
        "sass": "1.43.2"
    }
}
